    {
        "id": "d0e4b4b022b01533",
        "type": "function",
        "z": "72bd9c3b191aab5b",
        "name": "Generate Increasing/Decreasing AI Value-Function Node",
        "func": "var step = 5;\nvar minValue = 0;\nvar maxValue = 100;\nif (context.direction === undefined) {\n    context.direction = 1;\n    context.value = minValue;\n}\ncontext.value += context.direction * step;\nif (context.value >= maxValue) {\n    context.value = maxValue;\n    context.direction = -1;\n} else if (context.value <= minValue) {\n    context.value = minValue;\n    context.direction = 1;\n}\nmsg.payload = context.value;\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
        "initialize": "",
        "finalize": "",
        "libs": [],
        "x": 310,
